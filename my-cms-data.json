  "parts": [
    {
      "type": "header",
      "description": "ヘッダーコンポーネント",
      "module": [
        {
          "title": "site_header",
          "category": "common",
          "description": "サイトヘッダー",
          "tags": ["header", "navigation"],
          "body": "<header class=\"site-header ($bgColor:bg-white|bg-light|bg-dark)\">\n  <div class=\"container\">\n    <div class=\"logo\">\n      <img src=\"{$logoUrl:./img/logo.png}\" alt=\"{$logoAlt:ロゴ}\">\n    </div>\n    <nav class=\"main-nav\">\n      <a href=\"#\">{$nav1:ホーム}</a>\n      <a href=\"#\">{$nav2:サービス}</a>\n      <a href=\"#\">{$nav3:会社情報}</a>\n      <a href=\"#\">{$nav4:お問い合わせ}</a>\n    </nav>\n  </div>\n</header>"
        }
      ]
    },
    {
      "type": "hero",
      "description": "ヒーローセクション",
      "module": [
        {
          "title": "hero_section",
          "category": "common",
          "description": "メインヒーロー",
          "tags": ["hero", "main"],
          "body": "<section class=\"hero ($bgStyle:gradient|solid|image)\">\n  <div class=\"hero-content\">\n    <h1>{$title:Welcome to Our Site}</h1>\n    <p>{$subtitle:素晴らしい体験をお届けします}</p>\n    <button class=\"($btnStyle:btn-primary|btn-secondary|btn-outline)\">{$btnText:今すぐ始める}</button>\n  </div>\n  <div class=\"hero-image\">\n    <img src=\"{$heroImage:./img/hero_image.jpg}\" alt=\"{$imageAlt:ヒーロー画像}\">\n  </div>\n</section>"
        }
      ]
    },
    {
      "type": "content",
      "description": "コンテンツセクション",
      "module": [
        {
          "title": "text_block",
          "category": "common",
          "description": "テキストブロック",
          "tags": ["content", "text"],
          "body": "<div class=\"text-block\">\n  <h2>{$heading:見出しテキスト}</h2>\n  <p>{$content:ここに本文が入ります。重要な情報を分かりやすく伝えましょう。}</p>\n</div>"
        },
        {
          "title": "feature_cards",
          "category": "common",
          "description": "特徴カード",
          "tags": ["content", "cards", "features"],
          "body": "<div class=\"feature-cards\">\n  <h2>{$sectionTitle:特徴}</h2>\n  <div class=\"cards-grid\" z-for=\"features\">\n    <div class=\"card\">\n      <h3>($title:特徴タイトル)</h3>\n      <p>($description:特徴の説明)</p>\n    </div>\n  </div>\n</div>"
        }
      ]
    },
    {
      "type": "footer",
      "description": "フッターコンポーネント",
      "module": [
        {
          "title": "site_footer",
          "category": "common",
          "description": "サイトフッター",
          "tags": ["footer"],
          "body": "<footer class=\"site-footer ($bgColor:bg-dark|bg-light|bg-primary)\">\n  <div class=\"container\">\n    <div class=\"footer-content\">\n      <div class=\"footer-section\">\n        <h3>{$companyName:会社名}</h3>\n        <p>{$companyDesc:会社の説明}</p>\n      </div>\n      <div class=\"footer-section\">\n        <h3>リンク</h3>\n        <ul z-for=\"footerLinks\">\n          <li><a href=\"#\">($linkText:リンクテキスト)</a></li>\n        </ul>\n      </div>\n      <div class=\"footer-section\">\n        <h3>お問い合わせ</h3>\n        <p>{$email:info@example.com}</p>\n        <p>{$phone:03-1234-5678}</p>\n      </div>\n    </div>\n    <div class=\"footer-bottom\">\n      <p>&copy; {$year:2024} {$copyright:All rights reserved.}</p>\n    </div>\n  </div>\n</footer>"
        }
      ]
    }
  ],
